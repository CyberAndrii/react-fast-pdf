{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "expensify",
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:react/recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": [
        "dist/**.*",
        "node_modules/**",
        ".git/**",
        "webpack.config.js"
    ],
    "plugins": [
        "@typescript-eslint",
        "react"
    ],
    "rules": {},
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
